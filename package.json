{
  "name": "@consento/notification-server",
  "version": "0.1.6",
  "description": "A expo based notification implementation for Consento. Both server & client code. ",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "src/bin/consento-notify",
    "watch-node": "nodemon dist/bin/consento-notify",
    "ci": "npm run lint && npm run test -- --no-watchman",
    "test": "jest",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "pack": "pack-ts && rexreplace '#!/usr/bin/env npx ts-node' '#!/usr/bin/env node' dist/bin/consento-notify.js && chmod +x dist/bin/consento-notify.js",
    "release": "npm run ci && npm run build-ts && npm run pack && cd dist && npm publish --access=public",
    "lint": "eslint --ext .js,.ts ."
  },
  "author": "Martin Heidegger <martin.heidegger@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@consento/api": "^0.3.5",
    "@consento/crypto": "^0.3.4",
    "@extra-iterable/join": "^2.5.12",
    "@extra-iterable/map": "^2.5.12",
    "compare-versions": "^3.6.0",
    "cross-fetch": "^3.0.5",
    "events": "^3.2.0",
    "expo-constants": "^9.1.1",
    "expo-notifications": "^0.4.0",
    "expo-permissions": "^9.0.1",
    "expo-server-sdk": "^3.5.1",
    "express": "^4.17.1",
    "flexlock-cb": "^2.2.0",
    "get-random-values-polypony": "^1.0.0",
    "hyperdb": "^3.5.0",
    "hyperswarm": "^2.15.0",
    "level": "^6.0.1",
    "level-ttl": "^3.1.1",
    "p-map": "^4.0.0",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "rimraf": "^3.0.2",
    "url": "^0.11.0",
    "url-parse": "^1.4.7",
    "ws": "^7.3.1"
  },
  "bin": {
    "consento-notify": "bin/consento-notify.js"
  },
  "eslintConfig": {
    "extends": "standard-with-typescript",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "eslintIgnore": [
    "node_modules/*",
    "dist/*",
    "coverage/*",
    "**/*.d.ts",
    "/src/public/",
    "/src/types/"
  ],
  "devDependencies": {
    "@leichtgewicht/pack-ts": "^1.0.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/lodash.map": "^4.6.13",
    "@types/node": "^14.0.26",
    "@types/react-native": "^0.63.2",
    "@types/rimraf": "^3.0.0",
    "@types/tape": "^4.13.0",
    "@types/url-parse": "^1.4.3",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@unimodules/core": "^5.3.0",
    "@unimodules/react-native-adapter": "^5.4.0",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^7.5.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "jest-expo": "^38.0.2",
    "jest-react-native": "^18.0.0",
    "nodemon": "^2.0.4",
    "rexreplace": "^5.3.0",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7",
    "unimodules-permissions-interface": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/consento-org/notification-server.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/consento-org/notification-server/issues"
  },
  "homepage": "https://github.com/consento-org/notification-server#readme"
}
